{
  "name": "@blokwise/blok",
  "version": "2.0.0-2",
  "description": "Dynamically load components based on storyblok blok schemas in your nuxt.js project.",
  "author": "Julian Derungs <julian.derungs@deepbase.ch>",
  "license": "MIT",
  "repository": "blokwise/blok",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.com"
  },
  "keywords": [
    "nuxtjs",
    "nuxt/components",
    "storyblok",
    "blokwise"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint . --ext .ts,.vue",
    "format-check": "npx prettier --check ./",
    "format-write": "npx prettier --write ./",
    "test:e2e": "vitest run test/e2e"
  },
  "dependencies": {
    "@blokwise/dynamic": "2.0.0-6",
    "@nuxt/kit": "^3.0.0-rc.11",
    "consola": "^2.15.3"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxt/test-utils-edge": "3.0.0-rc.11-27727864.720da64",
    "@nuxtjs/eslint-config-typescript": "latest",
    "eslint": "latest",
    "jest": "^28.1.3",
    "nuxt": "3.0.0-rc.11",
    "vitest": "^0.22.0"
  }
}
